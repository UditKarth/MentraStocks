{
  "name": "stocktracker-app",
  "version": "1.0.0",
  "description": "StockTracker application for smart glasses using @mentra/sdk",
  "main": "src/app/StockTrackerApp.ts",
  "scripts": {
    "start": "node railway-start.js",
    "start:mock": "ts-node src/mock/StockTrackerApp-mock.ts",
    "dev": "tsx watch src/app/StockTrackerApp.ts",
    "dev:mock": "tsx watch src/mock/StockTrackerApp-mock.ts",
    "build": "tsc",
    "test": "ts-node test-runner.ts mock",
    "test:mock": "ts-node test-runner.ts mock",
    "test:api": "ts-node test-runner.ts api",
    "test:all": "ts-node test-runner.ts all",
    "test:real": "ts-node src/test-real-api.ts",
    "test:lookup": "ts-node src/test-company-lookup.ts",
    "test:database": "ts-node src/test-ticker-database.ts",
    "test:integration": "ts-node src/test-integration.ts",
    "verify": "ts-node src/verify-app.ts",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "bun:start": "bun run src/app/StockTrackerApp.ts",
    "bun:start:mock": "bun run src/mock/StockTrackerApp-mock.ts",
    "bun:dev": "bun --watch src/app/StockTrackerApp.ts",
    "bun:dev:mock": "bun --watch src/mock/StockTrackerApp-mock.ts",
    "bun:test": "bun run src/mock/test-mock-data.ts",
    "bun:test:real": "bun run src/test-real-api.ts",
    "bun:test:lookup": "bun run src/test-company-lookup.ts",
    "bun:verify": "bun run src/verify-app.ts"
  },
  "keywords": [
    "mentra",
    "smart-glasses",
    "stock-tracker",
    "typescript",
    "ar"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@mentra/sdk": "^2.1.12",
    "axios": "^1.6.0",
    "dotenv": "^17.2.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
